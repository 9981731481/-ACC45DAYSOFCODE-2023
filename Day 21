// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int w,x,y,z;
    printf(" enter the w");
    scanf("%d",&w);
    printf("enter the x");
    scanf("%d",&x);
    printf("enter the y");
    scanf("%d",&y);
    
    z=w+x*y;
    printf("%d",z);
    
return 0;
}
